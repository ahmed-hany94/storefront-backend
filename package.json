{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "Udacity FWD's second project",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "build": "npx tsc",
    "build-and-run": "npx tsc && node ./build/index.js",
    "jasmine": "set ENV=test && db-migrate --env test up && set PORT=3001 && jasmine && db-migrate db:drop test",
    "test": "yarn run build && yarn run jasmine",
    "lint": "eslint . --config .eslintrc --ext .ts",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "check": "yarn run prettier && yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmed-hany94/storefront-backend.git"
  },
  "bugs": {
    "url": "https://github.com/ahmed-hany94/storefront-backend/issues"
  },
  "homepage": "https://github.com/ahmed-hany94/storefront-backend#readme",
  "author": "Ahmed Hany",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^18.11.18",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
